{
  "apiVersion": "v1",
  "kind": "Pod",
  "metadata": {
    "name": "test-pod",
    "namespace": "default",
    "uid": "test-pod-uid",
    "ownerReferences": [
      {
        "apiVersion": "apps/v1",
        "kind": "ReplicaSet",
        "name": "test-replicaset",
        "uid": "test-replicaset-uid"
      }
    ],
    "labels": {
      "app": "test-app"
    }
  },
  "spec": {
    "volumes": [
      {
        "name": "config",
        "configMap": {
          "name": "test-configmap"
        }
      }
    ]
  }
}
